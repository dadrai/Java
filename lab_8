 2) import java.util.Scanner;

public class main {
  public static void main(String[] args) {
    
    System.out.println("код 1 = " + (int)'1' 
      + " код A = " + (int)'A' 
      + " код B = " + (int)'B'
      + " код a = " + (int)'a' 
      + " код b = " + (int)'b');  
    
     System.out.println("символ 40 = " + (char)40 
      + " символ 59 = " + (char)59 
      + " символ 79 = " + (char)79
      + " символ 85 = " + (char)85 
      + " символ 90 = " + (char)90);  
    
   System.out.println("символ 0X40 = " + (char)0X40 
      + " символ 0X5A = " + (char)0X5A 
      + " символ 0X71 = " + (char)0X71
      + " символ 0X72 = " + (char)0X72 
      + " символ 0X7A = " + (char)0X7A);
     

  } 
}

