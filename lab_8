import java.util.Scanner;

public class main {
  public static void main(String[] args) { 
    
System.out.println(1 + "Welcome " + 1 + 1);
System.out.println(1 + "Welcome " + (1 + 1));
System.out.println(1 + "Welcome " + ('\u0001' + 1));
System.out.println(1 + "Welcome " + 'a' + 1);

  }
}

