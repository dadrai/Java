import java.util.Scanner;
public class MyClass {
    public static void main(String args[]) {
         Scanner in = new Scanner(System.in);
        System.out.println("Vvedite n, for n!");
      int n=in.nextInt();
      int x=1;
      for (int i=1;i<=n;i++){x=x*i;}
          System.out.println(n+"!"+" = " + x );
      }

      
    
}
