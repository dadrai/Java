import java.util.Scanner;
public class MyClass {
    public static void main(String args[]) {
         Scanner in = new Scanner(System.in);
        System.out.println("Vvedite n, for 2^n");
      int n=in.nextInt();
      int x=2;
      
          System.out.println("2^"+n+" = " + Math.pow(x,n) );
     
      
    }
}
